{
  "name": "vime-oauth",
  "description": "Библиотека для работы с OAUTH-авторизацией на VimeWorld",
  "version": "1.0.7",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && echo {\"type\":\"commonjs\"} > cjs/package.json"
  },
  "keywords": [
    "vime",
    "vimeworld",
    "types",
    "typescript"
  ],
  "author": "Congiary",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "typescript": "^5.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VimeWorld-Hub/vime-oauth.git"
  },
  "bugs": {
    "url": "https://github.com/VimeWorld-Hub/vime-oauth/issues"
  },
  "files": [
    "src",
    "dist",
    "cjs",
    "README.md"
  ]
}
